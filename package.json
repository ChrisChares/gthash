{
  "name": "geotemporalhash",
  "version": "0.1.0",
  "description": "A hash of time and space on Earth with variable precision.",
  "author": "Chris Chares",
  "license": "MIT",
  "private": false,
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.11",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  }
}
